{
  "name": "broxie-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "everscale-inpage-provider": "^0.3.34",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-intl": "^6.0.5",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll-parallax": "^3.2.0",
    "swiper": "^8.3.2"
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "generate-intl": "formatjs extract \"src/**/*.tsx\" --format simple --out-file src/assets/lang/landing/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.7",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.7",
    "env-cmd": "^10.1.0",
